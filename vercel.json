{
    "functions": {
      "api/*.js": {
        "runtime": "nodejs@18.x"
      }
    },
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }